{
  "name": "@payloadcms/plugin-elevenlabs",
  "version": "0.1.0",
  "description": "ElevenLabs voice integration for Payload CMS - text-to-speech and speech-to-text",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "payload": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "payload",
    "cms",
    "elevenlabs",
    "voice",
    "tts",
    "text-to-speech",
    "speech-to-text",
    "audio",
    "plugin"
  ],
  "license": "MIT"
}
